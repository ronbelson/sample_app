<!DOCTYPE html>
<html>
    <head>
    <title><%= title %></title>
    <%= render 'layouts/stylesheets' %>
    </head>
    <body>
        <div class="container">
          <%= render 'layouts/header' %>
          <section class="round">
	        <% flash.each do |key ,value| %>
	          <div class="<%= key %>" ><%= value %></div>
	        <% end %>
            <%= yield %>
          </section>
          <%= render 'layouts/footer' %>
        </div>
    </body>
</html>
