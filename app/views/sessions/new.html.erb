<h1>Signin</h1>

<%= form_for(:session, :url => sessions_path) do |f| %>
	<div class="field">
	<%= f.label :email%><br/>
	<%= f.text_field :email%>
	</div>
	<div class="field">
	<%= f.label :password%><br/>
	<%= f.password_field :password%>
	</div>
	<div>
	<%= f.submit "Signin"%>
	</div>
<% end%>
<br/>
<p>Want to join us?  <%= link_to "Signup" , signup_path%></p>